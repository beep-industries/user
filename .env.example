# Database Configuration (PostgreSQL for user service)
DATABASE_URL=postgres://userservice:userservice_password@localhost:5432/userservice_db

# Server Configuration
SERVER_HOST=0.0.0.0
SERVER_PORT=3000

# Keycloak Configuration
# Note: Keycloak runs on port 8080, so user service uses port 3000
KEYCLOAK_URL=http://localhost:8080
KEYCLOAK_REALM=myrealm
KEYCLOAK_CLIENT_ID=user-service
KEYCLOAK_CLIENT_SECRET=your-client-secret-from-keycloak

# JWT Configuration
JWT_SECRET=your-jwt-secret-key
